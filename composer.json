{
  "name": "voodoo-mobile/yii2-pushes",
  "type": "library",
  "description": "Library for working with Amazon and Xtreme pushes",
  "keywords": [
    "push",
    "pushes",
    "aps",
    "gcm"
  ],
  "homepage": "https://github.com/voodoo-mobile/yii2-pushes",
  "license": "MIT",
  "authors": [
    {
      "name": "Voodoo Mobile Ltd",
      "email": "public@voodoo-mobile.com",
      "homepage": "http://voodoo-mobile.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0",
    "php-curl-class/php-curl-class": "~4.6",
    "aws/aws-sdk-php": "^3.15"
  },
  "autoload": {
    "psr-0": {
      "Pushes": "src"
    },
    "psr-4": {
      "vm\\pushes\\": "src/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    },
    "writable": [
    ]
  }
}